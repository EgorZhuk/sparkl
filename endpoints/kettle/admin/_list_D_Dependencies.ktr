//       "type": "ResourceFile"
//      }
//     ]
// 

	JSONArray rows = (JSONArray) JSONValue.parse(resultStr);

	Iterator it = rows.iterator();
	while (it.hasNext()) {

		JSONObject row = (JSONObject) it.next();
		String type_row = (String) row.get("type");
		JSONArray properties = (JSONArray) row.get("properties");

		if (!type_row.equals("LayoutResourceFile")) continue;

			Iterator it2 = properties.iterator();
